{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "We are beginning to migrate the KnetMiner REST API and Java code to [Apollo-GraphQL](https://www.apollographql.com/) and Neo4j. Our goal is to remove the dependency to the legacy OndexGraph Java library which is highly rooted in KnetMiner but not supoorted any longer.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js --watch .env --watch schema.graphql --watch index.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rothamsted/graphql-api.git"
  },
  "keywords": [],
