  "bugs": {
    "url": "https://github.com/Rothamsted/graphql-api/issues"
  },
  "homepage": "https://github.com/Rothamsted/graphql-api#readme",
  "dependencies": {
    "@neo4j/graphql": "^1.0.2",
    "apollo-server": "^2.25.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "neo4j-driver": "^4.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.9"
  }
}
